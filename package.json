{
  "name": "gatsby-starter-carraway",
  "description": "A GatsbySJ starter with typescript, react testing library and some sensible defaults",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endymion1818/gatsby-starter-carraway.git"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write '**/*.{js, jsx, ts, tsx}'",
    "test": "jest --config ./etc/test/jest/config.js",
    "lint": "tslint -c ./tslint.json 'src/**.{ts, tsx}'",
    "lh": "lighthousebot"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test",
      "pre-commit": "lint-staged -c ./lintstaged.json"
    }
  },
  "lighthouse": {
    "requiredScores": {
      "performance": 95,
      "accessibility": 90,
      "best-practices": 80,
      "seo": 90
    },
    "maxBundleSizeKb": 250,
    "jsBundleRegex": "app-[a-z0-9_]*.js"
  },
  "dependencies": {
    "gatsby": "^2.7.5",
    "gatsby-awesome-pagination": "^0.3.4",
    "gatsby-image": "^2.1.2",
    "gatsby-link": "^2.1.1",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-feed": "^2.2.3",
    "gatsby-plugin-lunr": "^1.5.1",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify-cms": "^4.0.1",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^2.1.3",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-twitter": "^2.0.13",
    "gatsby-plugin-webpack-size": "^0.0.3",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-transformer-javascript-frontmatter": "^2.0.10",
    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.21",
    "jest-dom": "^3.4.0",
    "jest-styled-components": "^6.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.2.1"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.133",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.15",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "gatsby-plugin-typescript": "^2.0.15",
    "gatsby-remark-copy-linked-files": "^2.0.13",
    "gatsby-remark-images": "^3.0.14",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lighthousebot": "git+https://github.com/GoogleChromeLabs/lighthousebot.git",
    "lint-staged": "^8.1.7",
    "lodash": "^4.17.11",
    "prettier": "^1.17.1",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1"
  }
}
