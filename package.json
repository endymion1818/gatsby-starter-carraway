{
  "name": "gatsby-starter-carraway",
  "description": "A GatsbySJ starter with typescript, react testing library and some sensible defaults",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endymion1818/gatsby-starter-carraway.git"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write '**/*.{js, jsx, ts, tsx}'",
    "test": "jest --config ./etc/test/jest/config.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn typecheck && yarn lint && yarn test --watchAll=false",
      "pre-commit": "lint-staged -c lintstaged.json"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.7.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-preset-gatsby": "^0.4.2",
    "gatsby": "^2.21.33",
    "gatsby-awesome-pagination": "^0.3.6",
    "gatsby-image": "^2.4.3",
    "gatsby-link": "^2.4.2",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-feed": "^2.5.1",
    "gatsby-plugin-lunr": "^1.5.1",
    "gatsby-plugin-manifest": "^2.4.3",
    "gatsby-plugin-offline": "^3.2.2",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^2.6.3",
    "gatsby-plugin-sitemap": "^2.4.2",
    "gatsby-plugin-styled-components": "^3.3.1",
    "gatsby-plugin-twitter": "^2.3.1",
    "gatsby-plugin-webpack-size": "^1.0.0",
    "gatsby-source-filesystem": "^2.3.3",
    "gatsby-transformer-javascript-frontmatter": "^2.3.2",
    "gatsby-transformer-json": "^2.4.2",
    "gatsby-transformer-remark": "^2.8.8",
    "gatsby-transformer-sharp": "^2.5.2",
    "jest-dom": "^4.0.0",
    "jest-styled-components": "^7.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-form-with-constraints": "^0.15.0",
    "react-helmet": "^6.0.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.2",
    "@types/lodash": "^4.14.151",
    "@types/lunr": "^2.3.3",
    "@types/react-helmet": "^6.0.0",
    "@types/styled-components": "^5.1.0",
    "axe-core": "^3.5.3",
    "babel-jest": "^26.0.1",
    "cypress": "^4.5.0",
    "cypress-axe": "^0.8.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-remark-copy-linked-files": "^2.3.2",
    "gatsby-remark-images": "^3.3.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "lodash": "^4.17.11",
    "prettier": "^2.0.5",
    "start-server-and-test": "^1.11.0",
    "typescript": "^3.9.2"
  }
}
