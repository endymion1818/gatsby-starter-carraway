{
  "name": "gatsby-starter-carraway",
  "description": "A GatsbySJ starter with typescript, react testing library and some sensible defaults",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endymion1818/gatsby-starter-carraway.git"
  },
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write '**/*.{js, jsx, ts, tsx}'",
    "test": "jest --config ./etc/test/jest/config.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn typecheck && yarn lint && yarn test --watchAll=false",
      "pre-commit": "lint-staged -c lintstaged.json"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-preset-gatsby": "^1.5.0",
    "gatsby": "^3.5.0",
    "gatsby-awesome-pagination": "^0.3.8",
    "gatsby-image": "^3.5.0",
    "gatsby-link": "^3.5.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-feed": "^3.5.0",
    "gatsby-plugin-lunr": "^1.5.1",
    "gatsby-plugin-manifest": "^3.5.0",
    "gatsby-plugin-offline": "^4.5.0",
    "gatsby-plugin-react-helmet": "^4.5.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^3.5.0",
    "gatsby-plugin-sitemap": "^4.1.0",
    "gatsby-plugin-styled-components": "^4.5.0",
    "gatsby-plugin-twitter": "^3.5.0",
    "gatsby-plugin-webpack-size": "^2.0.1",
    "gatsby-source-filesystem": "^3.5.0",
    "gatsby-transformer-javascript-frontmatter": "^3.5.0",
    "gatsby-transformer-json": "^3.5.0",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^3.5.0",
    "jest-dom": "^4.0.0",
    "jest-styled-components": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-form-with-constraints": "^0.16.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.169",
    "@types/lunr": "^2.3.3",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.9",
    "axe-core": "^4.2.0",
    "babel-jest": "^26.6.3",
    "cypress": "^7.3.0",
    "cypress-axe": "^0.12.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "gatsby-plugin-typescript": "^3.5.0",
    "gatsby-remark-copy-linked-files": "^4.2.0",
    "gatsby-remark-images": "^5.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.21",
    "start-server-and-test": "^1.12.2",
    "typescript": "^4.2.4"
  }
}
